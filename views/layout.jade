doctype html
html(lang='en')
  head
    // Required meta tags
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
    title Tabulampot
    // plugins:css
    link(rel='stylesheet', href='vendors/iconfonts/mdi/css/materialdesignicons.min.css')
    link(rel='stylesheet', href='vendors/css/vendor.bundle.base.css')
    // endinject
    // inject:css
    link(rel='stylesheet', href='css/style.css')
    // endinject
    link(rel='shortcut icon', href='images/favicon.png')
  body
    .container-scroller
      // partial:partials/_navbar.html
      nav.navbar.default-layout-navbar.col-lg-12.col-12.p-0.fixed-top.d-flex.flex-row
        .text-center.navbar-brand-wrapper.d-flex.align-items-center.justify-content-center
          a.navbar-brand.brand-logo(href='/')
            img(src='images/logo.svg', alt='logo')
          a.navbar-brand.brand-logo-mini(href='index.html')
            img(src='images/logo-mini.svg', alt='logo')
        .navbar-menu-wrapper.d-flex.align-items-stretch
          .search-field.d-none.d-md-block
            form.d-flex.align-items-center.h-100(action='#')
              .input-group
                .input-group-prepend.bg-transparent
                  i.input-group-text.border-0.mdi.mdi-magnify
                input.form-control.bg-transparent.border-0(type='text', placeholder='Search projects')
          ul.navbar-nav.navbar-nav-right
            li.nav-item.nav-profile.dropdown
              a#profileDropdown.nav-link.dropdown-toggle(href='#', data-toggle='dropdown', aria-expanded='false')
                .nav-profile-img
                  img(src='images/faces/face1.jpg', alt='image')
                  span.availability-status.online
                .nav-profile-text
                  p.mb-1.text-black Shela Ernita Sari
              .dropdown-menu.navbar-dropdown(aria-labelledby='profileDropdown')
                a.dropdown-item(href='#')
                  i.mdi.mdi-cached.mr-2.text-success
                  |                 Activity Log
                .dropdown-divider
                a.dropdown-item(href='#')
                  i.mdi.mdi-logout.mr-2.text-primary
                  |                 Signout
            li.nav-item.d-none.d-lg-block.full-screen-link
              a.nav-link
                i#fullscreen-button.mdi.mdi-fullscreen
            li.nav-item.dropdown
              a#messageDropdown.nav-link.count-indicator.dropdown-toggle(href='#', data-toggle='dropdown', aria-expanded='false')
                i.mdi.mdi-email-outline
                span.count-symbol.bg-warning
              .dropdown-menu.dropdown-menu-right.navbar-dropdown.preview-list(aria-labelledby='messageDropdown')
                h6.p-3.mb-0 Messages
                .dropdown-divider
                a.dropdown-item.preview-item
                  .preview-thumbnail
                    img.profile-pic(src='images/faces/face4.jpg', alt='image')
                  .preview-item-content.d-flex.align-items-start.flex-column.justify-content-center
                    h6.preview-subject.ellipsis.mb-1.font-weight-normal Mark send you a message
                    p.text-gray.mb-0
                      | 1 Minutes ago
                .dropdown-divider
                a.dropdown-item.preview-item
                  .preview-thumbnail
                    img.profile-pic(src='images/faces/face2.jpg', alt='image')
                  .preview-item-content.d-flex.align-items-start.flex-column.justify-content-center
                    h6.preview-subject.ellipsis.mb-1.font-weight-normal Cregh send you a message
                    p.text-gray.mb-0
                      | 15 Minutes ago
                .dropdown-divider
                a.dropdown-item.preview-item
                  .preview-thumbnail
                    img.profile-pic(src='images/faces/face3.jpg', alt='image')
                  .preview-item-content.d-flex.align-items-start.flex-column.justify-content-center
                    h6.preview-subject.ellipsis.mb-1.font-weight-normal Profile picture updated
                    p.text-gray.mb-0
                      | 18 Minutes ago
                .dropdown-divider
                h6.p-3.mb-0.text-center 4 new messages
            li.nav-item.dropdown
              a#notificationDropdown.nav-link.count-indicator.dropdown-toggle(href='#', data-toggle='dropdown')
                i.mdi.mdi-bell-outline
                span.count-symbol.bg-danger
              .dropdown-menu.dropdown-menu-right.navbar-dropdown.preview-list(aria-labelledby='notificationDropdown')
                h6.p-3.mb-0 Notifications
                .dropdown-divider
                a.dropdown-item.preview-item
                  .preview-thumbnail
                    .preview-icon.bg-success
                      i.mdi.mdi-calendar
                  .preview-item-content.d-flex.align-items-start.flex-column.justify-content-center
                    h6.preview-subject.font-weight-normal.mb-1 Event today
                    p.text-gray.ellipsis.mb-0
                      | Just a reminder that you have an event today
                .dropdown-divider
                a.dropdown-item.preview-item
                  .preview-thumbnail
                    .preview-icon.bg-warning
                      i.mdi.mdi-settings
                  .preview-item-content.d-flex.align-items-start.flex-column.justify-content-center
                    h6.preview-subject.font-weight-normal.mb-1 Settings
                    p.text-gray.ellipsis.mb-0
                      | Update dashboard
                .dropdown-divider
                a.dropdown-item.preview-item
                  .preview-thumbnail
                    .preview-icon.bg-info
                      i.mdi.mdi-link-variant
                  .preview-item-content.d-flex.align-items-start.flex-column.justify-content-center
                    h6.preview-subject.font-weight-normal.mb-1 Launch Admin
                    p.text-gray.ellipsis.mb-0
                      | New admin wow!
                .dropdown-divider
                h6.p-3.mb-0.text-center See all notifications
            li.nav-item.nav-logout.d-none.d-lg-block
              a.nav-link(href='#')
                i.mdi.mdi-power
            li.nav-item.nav-settings.d-none.d-lg-block
              a.nav-link(href='#')
                i.mdi.mdi-format-line-spacing
          button.navbar-toggler.navbar-toggler-right.d-lg-none.align-self-center(type='button', data-toggle='offcanvas')
            span.mdi.mdi-menu
      // partial
      .container-fluid.page-body-wrapper
        // partial:partials/_sidebar.html
        nav#sidebar.sidebar.sidebar-offcanvas
          ul.nav
            li.nav-item.nav-profile
              a.nav-link(href='#')
                .nav-profile-image
                  img(src='images/faces/face1.jpg', alt='profile')
                  span.login-status.online
                  // change to offline or busy as needed
                .nav-profile-text.d-flex.flex-column
                  span.font-weight-bold.mb-2 Shela Ernita Sari
                  span.text-secondary.text-small Admin
                i.mdi.mdi-bookmark-check.text-success.nav-profile-badge
            li.nav-item
              a.nav-link(href='/')
                span.menu-title Dashboard
                i.mdi.mdi-home.menu-icon
            li.nav-item
              a.nav-link(href='/chart')
                span.menu-title Charts
                i.mdi.mdi-chart-bar.menu-icon
            li.nav-item
              a.nav-link(href='/insertmenu')
                span.menu-title Menu
                i.mdi.mdi-format-list-bulleted.menu-icon
            li.nav-item
              a.nav-link(href='/insertcontent')
                span.menu-title Content
                i.mdi.mdi-table-large.menu-icon
            li.nav-item
              a.nav-link(href='/insertadmin')
                span.menu-title Registrasi
                i.mdi.mdi-format-line-spacing.menu-icon
        // partial
        .main-panel
          div
            block content
          // content-wrapper ends
  
          // partial:partials/_footer.html
          footer.footer
            .d-sm-flex.justify-content-center.justify-content-sm-between
              span.text-muted.text-center.text-sm-left.d-block.d-sm-inline-block Copyright Â© 2018 . All rights reserved.
              span.float-none.float-sm-right.d-block.mt-1.mt-sm-0.text-center
                | Shela Ernita Sari & made with 
                i.mdi.mdi-heart.text-danger
          // partial
        // main-panel ends
      // page-body-wrapper ends
    // container-scroller
    // plugins:js
    script(src='vendors/js/vendor.bundle.base.js')
    script(src='vendors/js/vendor.bundle.addons.js')
    // endinject
    // Plugin js for this page
    // End plugin js for this page
    // inject:js
    script(src='js/off-canvas.js')
    script(src='js/misc.js')
    // endinject
    // Custom js for this page
    script(src='js/dashboard.js')
    // End custom js for this page
    script(src='../../vendors/js/vendor.bundle.base.js')
    script(src='../../vendors/js/vendor.bundle.addons.js')
    // Custom js for this page
    script(src='../../js/chart.js')
    script(src='../../js/off-canvas.js')
    script(src='../../js/file-upload.js')

